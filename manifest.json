{
    "manifest_version": 2,
    "name": "Vim Text Editor",
    "version": "1.0",
    "description": "Edit text with Vim keybindings.",
    "icons": {
        "32": "icons/icon-32.png",
        "48": "icons/icon-48.png",
        "64": "icons/icon-64.png",
        "128": "icons/icon-128.png"
    },
    "content_scripts": [
        {
            "matches": [ "<all_urls>" ],
            "js": ["js/main.js"]
        }
    ]
}
